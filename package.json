{
  "name": "xtenni-devicecloud-client",
  "version": "1.0.0",
  "description": "A client web site for LMU message parsing service",
  "author": {
    "name": "Bora Eryilmaz"
  },
  "engines": {
    "node": "6.9.2"
  },
  "dependencies": {
    "express": "^4.14.0",
    "mongodb": "^2.2.10",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.5.1"
  },
  "devDependencies": {
    "jshint": "^2.9.2",
    "mocha": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BoraE/xtenni-devicecloud-client"
  },
  "scripts": {
    "start": "node app.js",
    "watch": "nodemon --ignore 'out' -w . -e js -x 'npm run build'",

    "build": "npm run build:js && npm run test",
    "build:js": "npm run jshint && npm run jsdoc",

    "jshint": "jshint . --exclude-path .gitignore",
    "jsdoc": "jsdoc -c conf.json -r ./",
    "test": "mocha"
  }
}
